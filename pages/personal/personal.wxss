/* pages/personal/personal.wxss */
.bg{
  width: 100%;
  height: 100%;
  position:fixed; 
  background-size:100% 100%;
  z-index: -1;
}
.cut{
  padding: 25rpx 0;
}
.person {
  width: 92%;
  height: 14vh;
  margin:5rpx  4%;
  background-color: white;
  border-radius: 5px 5px 5px 5px;
  display: flex;
  margin-top: 5rpx;
}

.headimage {
   width: 90rpx;
  height: 90rpx;
  border-radius: 70px 70px 70px 70px;
  margin: 40rpx 30rpx;
}


.headimage image {
    width: 90rpx;
  height: 90rpx;
  border-radius: 70px 70px 70px 70px;
}

.student {
  width: 80%;
}

.name {
  width: 100%;
  margin-top:30rpx;
  display: flex;
  font-weight: bold;
}

.role {
  width: 40px;
  height: 18px;
  font-size: 13px;
  text-align: center;
  border-radius: 5px 5px 5px 5px;
  margin-top: 16rpx;
  background-color: grey;
  color: white;
}
.center {
  width: 92%;
  margin:5rpx  4%;
  border-radius: 5px 5px 5px 5px;
  display: flex;
  margin-top: 5rpx;
}