<!--pages/personal/personal.wxml-->
<view class='leave'>
<image class="bg" src="https://suggestion.ujnxgzx.com/static/img/6.jpg"></image>
 <view class='person'>
    <view class='headimage'>
      <image src='{{userInfo.avatarUrl}}'></image>
    </view>
    <view class='student'>
      <view class='name'>{{userInfo.nickName}}
        </view>
      <view class='role'>老师</view>
    </view>
  </view>
<van-cell class="center" title="查看所有回复" is-link link-type="navigateTo"
  url='../details/details'/>
<view class="footer">
<van-tabbar active="{{active}}" bind:change="onChange">
  <van-tabbar-item icon="home-o">首页</van-tabbar-item>
  <van-tabbar-item icon="user-o">我的</van-tabbar-item>
</van-tabbar>
</view>
</view>