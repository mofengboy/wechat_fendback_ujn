<!--pages/personal/personal.wxml-->
<view class='leave'>
 <view class='person'>
    <view class='headimage'>
      <image src='{{userInfo.avatarUrl}}'></image>
    </view>
    <view class='student'>
      <view class='name'>{{userInfo.nickName}}
        </view>
      <view class='role'>学生</view>
    </view>
  </view>
<block  wx:for="{{info}}" wx:key="{{item.id}}">
<van-cell class="center" title="{{item.title}}" is-link link-type="navigateTo"
  url='../reply_recived/reply_recived?id={{item.id}}' value="{{item.time}}"/>
</block>
<view class="footer">
<van-tabbar active="{{active}}" bind:change="onChange">
  <van-tabbar-item icon="home-o">首页</van-tabbar-item>
  <van-tabbar-item icon="user-o">我的</van-tabbar-item>
</van-tabbar>
</view>
</view>